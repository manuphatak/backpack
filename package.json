{
  "name": "go-bag",
  "description": "A CLI tool to take your computer setup on the road.",
  "version": "0.0.5",
  "author": "Manu Phatak @manuphatak",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "manuphatak/go-bag",
  "types": "lib/index.d.ts",
  "bin": {
    "go-bag": "./bin/run"
  },
  "bugs": "https://github.com/manuphatak/go-bag/issues",
  "dependencies": {
    "@oclif/command": "1",
    "@oclif/config": "1",
    "@oclif/plugin-help": "3",
    "tslib": "2.1.0"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "10.16.8",
    "@auto-it/conventional-commits": "10.16.8",
    "@auto-it/first-time-contributor": "10.16.8",
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@oclif/dev-cli": "1",
    "@oclif/test": "1",
    "@tsconfig/node14": "1.0.0",
    "@types/chai": "4",
    "@types/mocha": "8",
    "@types/node": "14.14.31",
    "auto": "10.16.8",
    "chai": "4",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.21.0",
    "eslint-config-oclif": "3.1",
    "eslint-config-oclif-typescript": "0.2.0",
    "eslint-config-prettier": "8.1.0",
    "globby": "11.0.2",
    "husky": "5.1.1",
    "mocha": "8",
    "nyc": "15.1.0",
    "pinst": "2.1.6",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.2 "
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/manuphatak/go-bag",
  "keywords": [
    "oclif"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "go-bag",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "lint": "eslint . --ext .ts",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme && prettier --write README.md",
    "test": "COLUMNS=80 nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && prettier --write README.md && git add README.md && git commit --allow-empty -m 'chore: Update README.md [skip ci]'",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "release": "auto shipit"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "auto": {
    "plugins": [
      "npm",
      "all-contributors",
      "conventional-commits",
      "first-time-contributor",
      "released"
    ],
    "author": "Auto <auto@phatak.io>"
  }
}
