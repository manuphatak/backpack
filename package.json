{
  "name": "@manuphatak/backpack",
  "description": "A quick CLI tool to help hotswap your machine by backing up your local setup.",
  "version": "0.0.0",
  "author": "Manu Phatak @manuphatak",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "manuphatak/backpack",
  "types": "lib/index.d.ts",
  "bin": {
    "backpack": "./bin/run"
  },
  "bugs": "https://github.com/manuphatak/backpack/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "10.16.8",
    "@auto-it/conventional-commits": "10.16.8",
    "@auto-it/first-time-contributor": "10.16.8",
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "auto": "10.16.8",
    "chai": "^4",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "8.1.0",
    "globby": "^10",
    "husky": "5.1.1",
    "mocha": "^5",
    "nyc": "^14",
    "pinst": "2.1.6",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/manuphatak/backpack",
  "keywords": [
    "oclif"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "backpack",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme && prettier --write README.md",
    "test": "COLUMNS=80 nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && prettier --write README.md && git add README.md && git commit --allow-empty -m 'chore: Update README.md [skip ci]'",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "release": "auto shipit"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "auto": {
    "plugins": [
      "npm",
      "all-contributors",
      "conventional-commits",
      "first-time-contributor",
      "released"
    ],
    "author": "Auto <auto@phatak.io>"
  }
}
